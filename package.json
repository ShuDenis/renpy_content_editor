{
  "name": "renpy-content-editor",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "tsc -b --pretty",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "test": "vitest run",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.23.8",
    "reactflow": "^11.10.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@testing-library/jest-dom": "^6.2.2",
    "@testing-library/react": "^14.3.1",
    "jsdom": "^24.0.0",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0",
    "vite": "^5.4.0",
    "@vitejs/plugin-react": "^4.3.1",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.2",
    "eslint": "^8.57.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "eslint-config-prettier": "^9.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,css,html,json,md}": "prettier --write"
  }
}
